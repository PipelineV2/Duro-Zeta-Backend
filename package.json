  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "export NODE_ENV=prod && npm run build && node dist/server.js",
    "dev:watch": "export NODE_ENV=dev && npm run build && nodemon src/server.ts",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config --exec 'ts-node --files' src/server.ts",
    "dev": "npm run nodemon"

  },
